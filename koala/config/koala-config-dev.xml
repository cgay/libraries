<?xml version="1.0" encoding="ISO-8859-1"?>

<!-- This is a version of the configuration file that's more suitable for
     use during development.  Copy it to sources/example/config/koala-config.xml. -->

<koala>

<server-root location="." />

<mime-type-map location="mime-type-map.xml" />

<logfile location="koala.log" />

<port value="8001" />

<document-root location="www" />

<log level="copious" />

<debug-server value="on" />

<auto-register enabled="no" />

<xml-rpc url="/RPC2" enable="yes" internal-error-fault-code="0" />


</koala>
